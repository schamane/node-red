{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "sourceMap": false,
    "strict": false,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "declaration": true
  },
  "exclude": [
    "node_modules"
  ]
}